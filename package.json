{
  "name": "dogs-parks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "map": "vite build --sourcemap",
    "analyze": "SME=1 vite build --sourcemap && source-map-explorer 'dist/assets/index-*.js' 'dist/assets/index-*.js.map' --no-border-checks --html dist/bundle-report.html && (open dist/bundle-report.html || xdg-open dist/bundle-report.html || true)",
    "test": "vitest run",
    "ios-setup": "./.claude/commands/ios-dev-setup.sh",
    "pr": "BR=$(git rev-parse --abbrev-ref HEAD); git push -u origin $BR; TITLE=$(node scripts/pr-title.js $BR); gh pr create --base main --head $BR --title \"$TITLE\" --fill || (echo 'PR likely exists; showing current PR' && gh pr view --web || true)",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@capacitor-firebase/messaging": "7.3.0",
    "@capacitor/android": "7.4.3",
    "@capacitor/app": "7.0.2",
    "@capacitor/browser": "7.0.2",
    "@capacitor/camera": "7.0.2",
    "@capacitor/core": "7.4.3",
    "@capacitor/geolocation": "7.1.5",
    "@capacitor/ios": "7.4.3",
    "@capacitor/keyboard": "7.0.2",
    "@capacitor/network": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/screen-orientation": "7.0.2",
    "@capacitor/share": "7.0.2",
    "@capacitor/status-bar": "7.0.2",
    "@capawesome/capacitor-badge": "7.0.1",
    "@supabase/auth-ui-react": "0.4.7",
    "@supabase/auth-ui-shared": "0.1.8",
    "@supabase/supabase-js": "2.49.1",
    "@tanstack/react-query": "5.37.1",
    "@tanstack/react-virtual": "3.13.6",
    "browser-image-compression": "2.0.2",
    "capacitor-native-settings": "7.0.1",
    "capacitor-plugin-safe-area": "4.0.0",
    "classnames": "2.5.1",
    "dayjs": "1.11.11",
    "firebase": "11.10.0",
    "geolib": "3.3.4",
    "i18next": "^25.5.2",
    "leaflet": "1.9.4",
    "leaflet-control-geocoder": "2.4.0",
    "lucide-react": "0.488.0",
    "openrouteservice-js": "0.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.7.3",
    "react-intersection-observer": "9.16.0",
    "react-leaflet": "4.2.1",
    "react-leaflet-cluster": "2.1.0",
    "react-router-dom": "7.5.3",
    "react-slick": "0.30.2",
    "react-webcam": "7.2.0",
    "recharts": "2.12.6",
    "slick-carousel": "1.8.1",
    "uuid": "9.0.1",
    "vite-plugin-svgr": "4.3.0",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "7.4.3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.17",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.22",
    "@types/react-slick": "^0.23.13",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "sass": "^1.75.0",
    "source-map-explorer": "^2.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.2.3"
  }
